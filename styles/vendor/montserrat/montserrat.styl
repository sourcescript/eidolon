/*
 * Latin
 */

@font-face
  font-family 'Montserrat'
  font-style normal
  font-weight montserrat-weight-normal
  src local('Montserrat-Regular'), url(fonts + 'montserrat-normal.woff2') format('woff2')
  unicode-range unquote('U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215, U+E0FF, U+EFFD, U+F000')

@font-face
  font-family 'Montserrat'
  font-style normal
  font-weight montserrat-weight-bold
  src local('Montserrat-Bold'), url(fonts + 'montserrat-bold.woff2') format('woff2')
  unicode-range U+0000-00FF, U+0131, U+0152-0153, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2212, U+2215, U+E0FF, U+EFFD, U+F000

.u-montserrat-normal
  font-weight: montserrat-weight-normal  

.u-montserrat-bold
  font-weight: montserrat-weight-bold
